<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.csswust.patest2.dao.ProblemKnowledgeDao">
    <select id="selectByCondition" resultMap="BaseResultMap" parameterType="java.util.Map">
        SELECT
        <include refid="Base_Column_List"/>
        FROM problem_knowledge
        WHERE 1 = 1
        <if test="record.proKnoId != null">
            AND
            pro_kno_id = #{record.proKnoId}
        </if>
        <if test="record.probblemId != null">
            AND
            probblem_id = #{record.probblemId}
        </if>
        <if test="record.knowledgeId != null">
            AND
            knowledge_id = #{record.knowledgeId}
        </if>
        <if test="record.createTime != null">
            AND
            create_time = #{record.createTime}
        </if>
        <if test="record.modifyTime != null">
            AND
            modify_time = #{record.modifyTime}
        </if>
        <include refid="custom_condition"/>
        <if test="start != null and rows != null">
            LIMIT #{start}, #{rows}
        </if>
    </select>
    <select id="selectByConditionGetCount" resultType="java.lang.Integer" parameterType="java.util.Map">
        SELECT
        count(*)
        FROM problem_knowledge
        WHERE 1 = 1
        <if test="record.proKnoId != null">
            AND
            pro_kno_id = #{record.proKnoId}
        </if>
        <if test="record.probblemId != null">
            AND
            probblem_id = #{record.probblemId}
        </if>
        <if test="record.knowledgeId != null">
            AND
            knowledge_id = #{record.knowledgeId}
        </if>
        <if test="record.createTime != null">
            AND
            create_time = #{record.createTime}
        </if>
        <if test="record.modifyTime != null">
            AND
            modify_time = #{record.modifyTime}
        </if>
        <include refid="custom_condition"/>
        <if test="start != null and rows != null">
            LIMIT #{start}, #{rows}
        </if>
    </select>
    <delete id="deleteByIdsList" parameterType="java.util.Map">
        delete from problem_knowledge
        where pro_kno_id in
        <foreach collection="list" item="item" index="index" open="(" close=")" separator=",">
            #{item}
        </foreach>
    </delete>
    <select id="selectByIdsList" resultMap="BaseResultMap" parameterType="java.util.Map">
        select
        <include refid="Base_Column_List" />
        from problem_knowledge
        where pro_kno_id in
        <foreach collection="list" item="item" index="index" open="(" close=")" separator=",">
            #{item}
        </foreach>
    </select>
</mapper>