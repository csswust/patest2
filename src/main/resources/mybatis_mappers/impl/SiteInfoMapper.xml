<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.csswust.patest2.dao.SiteInfoDao">
    <select id="selectByCondition" resultMap="BaseResultMap" parameterType="java.util.Map">
        SELECT
        <include refid="Base_Column_List"/>
        ,
        <include refid="Blob_Column_List"/>
        FROM site_info
        WHERE 1 = 1
        <if test="record.siteId != null">
            AND
            site_id = #{record.siteId}
        </if>
        <if test="record.name != null">
            AND
            name = #{record.name}
        </if>
        <if test="record.extra != null">
            AND
            extra = #{record.extra}
        </if>
        <if test="record.createTime != null">
            AND
            create_time = #{record.createTime}
        </if>
        <if test="record.modifyTime != null">
            AND
            modify_time = #{record.modifyTime}
        </if>
        <if test="record.value != null">
            AND
            value = #{record.value}
        </if>
        <if test="start != null and rows != null">
            LIMIT #{start}, #{rows}
        </if>
    </select>
    <select id="selectByConditionGetCount" resultType="java.lang.Integer" parameterType="java.util.Map">
        SELECT
        count(*)
        FROM site_info
        WHERE 1 = 1
        <if test="record.siteId != null">
            AND
            site_id = #{record.siteId}
        </if>
        <if test="record.name != null">
            AND
            name = #{record.name}
        </if>
        <if test="record.extra != null">
            AND
            extra = #{record.extra}
        </if>
        <if test="record.createTime != null">
            AND
            create_time = #{record.createTime}
        </if>
        <if test="record.modifyTime != null">
            AND
            modify_time = #{record.modifyTime}
        </if>
        <if test="record.value != null">
            AND
            value = #{record.value}
        </if>
        <if test="start != null and rows != null">
            LIMIT #{start}, #{rows}
        </if>
    </select>
    <delete id="deleteByIdsList" parameterType="java.util.Map">
        delete from site_info
        where use_pro_id in
        <foreach collection="list" item="item" index="index" open="(" close=")" separator=",">
            #{item}
        </foreach>
    </delete>
</mapper>